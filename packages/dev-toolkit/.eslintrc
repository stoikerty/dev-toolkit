{
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [
      "error",
      {
        "ignore": ["^dist/"]
      }
    ],
    "import/extensions": ["off", "never"]
  },
  "env": {
    "mocha": true
  }
}
