{
  "plugins": [
    "jsx-control-statements"
  ],
  "extends": [
    "airbnb",
    "plugin:jsx-control-statements/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1
      }
    ],
    "import/no-unresolved": [
      "error",
      {
        "ignore": [ "src/" ]
      }
    ]
  },

  "globals": [
    "app"
  ],
  "env": {
    "browser": true,
    "mocha": true
  }
}
