{
  "name": "dev-toolkit-with-sass-template",
  "version": "1.0.0",
  "description": "universal app",
  "author": "",
  "scripts": {
    "start": "npm run build && npm run serve",
    "dev": "cross-env NODE_ENV=development dev-toolkit watch",
