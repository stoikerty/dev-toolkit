    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "dev-toolkit": "^7.0.3",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.8.1",
    "snyk": "^1.85.0"
  },
  "devDependencies": {
    "prettier": "^1.11.1"
  },
  "snyk": true
